@{
    DateTime d = ViewBag.EmployeeData.Birthdate;
    string s = d.ToString("yyyy-MM-dd");
}
<h1>
    <h2>資料內容:</h2>
    <h3>員工編號:@ViewBag.EmployeeData.Id</h3>
    <h3>員工姓名:@ViewBag.EmployeeData.Name</h3>
    <h3>出生年月日:@s</h3>
    <h3>薪資:@ViewBag.EmployeeData.Salary</h3>
</h1>